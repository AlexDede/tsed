    "@tsed/di": "7.0.0-alpha.2",
    "@tsed/openspec": "7.0.0-alpha.2",
    "@tsed/schema": "7.0.0-alpha.2"
  },
  "peerDependencies": {
    "@tsed/common": "^7.0.0-alpha.2",
    "@tsed/core": "^7.0.0-alpha.2",
    "@tsed/di": "^7.0.0-alpha.2",
    "@tsed/openspec": "^7.0.0-alpha.2",
    "@tsed/schema": "^7.0.0-alpha.2"
