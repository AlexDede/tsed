{
  "name": "@tsed/schema-formio",
  "version": "7.0.0-alpha.1",
  "description": "Transform Ts.ED Schema & JsonSchema to a valid Formio schema",
  "private": false,
  "source": "./src/index.ts",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "exports": {
    "require": "./lib/index.js",
    "default": "./lib/index.modern.js"
  },
  "keywords": [
    "TypeScript",
    "decorators",
    "models",
    "json",
    "schema",
    "JsonSchema",
    "formio",
    "class",
    "classes",
    "tsed"
  ],
  "scripts": {
    "build": "microbundle --target node --no-compress --format modern,cjs --tsconfig ./tsconfig.compile.json",
    "test": "cross-env NODE_ENV=test nyc mocha"
  },
  "dependencies": {
    "change-case": "4.1.2",
    "tslib": "2.2.0"
  },
  "devDependencies": {
    "@tsed/core": "7.0.0-alpha.1",
    "@tsed/openspec": "7.0.0-alpha.1",
    "@tsed/schema": "7.0.0-alpha.1"
  },
  "peerDependencies": {
    "@tsed/core": "^7.0.0-alpha.1",
    "@tsed/openspec": "^7.0.0-alpha.1",
    "@tsed/schema": "^7.0.0-alpha.1"
  }
}