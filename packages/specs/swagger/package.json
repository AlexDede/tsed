    "@tsed/json-mapper": "7.0.0-alpha.1",
    "@tsed/openspec": "7.0.0-alpha.1",
    "@tsed/schema": "7.0.0-alpha.1"
  },
  "peerDependencies": {
    "@tsed/common": "^7.0.0-alpha.1",
    "@tsed/core": "^7.0.0-alpha.1",
    "@tsed/di": "^7.0.0-alpha.1",
    "@tsed/json-mapper": "^7.0.0-alpha.1",
    "@tsed/openspec": "^7.0.0-alpha.1",
